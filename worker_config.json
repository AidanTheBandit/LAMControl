{
  "worker": {
    "name": "LAMControl-Worker",
    "location": "localhost",
    "description": "Auto-configured LAMControl worker",
    "port": 6000,
    "max_concurrent_tasks": 5
  },
  "server": {
    "endpoint": "http://localhost:8080"
  },
  "integrations": {
    "browser": {
      "enabled": true,
      "features": [
        "site_browsing",
        "google_search",
        "youtube_search"
      ],
      "settings": {
        "default_browser": "system_default"
      }
    },
    "computer": {
      "enabled": true,
      "features": [
        "volume_control",
        "media_control",
        "system_control"
      ],
      "settings": {
        "vol_step_value": 5
      }
    },
    "messaging": {
      "enabled": false,
      "features": [
        "discord",
        "telegram"
      ],
      "settings": {
        "auto_connect": false
      }
    },
    "ai": {
      "enabled": false,
      "features": [
        "openinterpreter",
        "automation"
      ],
      "settings": {
        "llm_model": "llama-3.3-70b-versatile",
        "auto_run": false
      }
    }
  }
}